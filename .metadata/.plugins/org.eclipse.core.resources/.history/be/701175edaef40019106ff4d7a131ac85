
public class test {

	public static void main(String[] args) {

		System.out.println("정수는 7, 실수는 3.14f, 문자열은 Love");
						
		System.out.printf("정수는 %d, 실수는 %.2f, 문자는 %c, 문자열은 %s \n", 7, 3.14f, 'J', "love");
		System.out.println("");
		
		String str = "love";
		
		System.out.printf("정수는 %d, 실수는 %.2f, 문자는 %c, 문자열은 %s \n", 7, 3.14f, 'J', "str");
		
	}

}
