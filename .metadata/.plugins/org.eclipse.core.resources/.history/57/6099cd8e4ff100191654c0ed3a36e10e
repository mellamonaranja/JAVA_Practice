
public class VariableTypeEx01 {
	public static void main(String [] args) {
		
		boolean bool;
		bool = true;
		
		System.out.println("bool = " + bool);
		
		bool = false;
		System.out.println("bool = " + bool);
		
//		bool = 20;
		char char_a1 = 'A';
		System.out.println("char_a1 =" + char_a1);
		
	}

}
